{
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "baseUrl": "./",
    "sourceMap": true,
    "target": "es6",
    "module": "commonJs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true
  }
}
